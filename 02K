#include <iostream>
using namespace std;
int main() {
    double num1, num2, num3, media;
	cout << "Insira o primeiro número: ";
	cin >> num1;
	cout << "Insira o segundo número: ";
	cin >> num2;
	cout << "Insira o terceiro número: ";
	cin >> num3;
	media = (num1 + num2 + num3) / 3;
	cout << "A média dos três números é: " << media;
	return 0;
}
