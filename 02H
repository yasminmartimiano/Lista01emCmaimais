#include <iostream>
using namespace std;
int main() {
    double num1, num2, multiplicacao;
	cout << "Insira o primeiro número: ";
	cin >> num1;
	cout << "Insira o segundo número: ";
	cin >> num2;
	multiplicacao = num1 * num2;
	cout << num1 << " * " << num2 << " = " << multiplicacao << ".";
	return 0;
}
