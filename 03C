#include <iostream>
using namespace std;
int main() {
  int num1, num2;
  cout << "Digite um número inteiro: ";
  cin >> num1;
  cout << "Digite outro número inteiro: ";
  cin >> num2;
  if (num1 == num2) {
      cout << "O número " << num1 << " é igual ao número " << num2 << "."; 
  } else {
      cout << "O número " << num1 << " não é igual ao número " << num2 << ".";
  }
    return 0;
}
